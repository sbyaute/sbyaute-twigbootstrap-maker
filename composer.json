{
    "name": "sbyaute/twigbootstrap-maker",
    "type": "symfony-bundle",
    "description": "TwigBootstrap Maker helps you create controllers and templates boostraped for twigbootstrap theme bundle",
    "keywords": [ "adminlte", "twigbootstrap", "crud" ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "symfony/twig-bundle": ">=4.4",
        "symfony/console": ">=4.4",
        "symfony/form": ">=4.4",
        "symfony/security-bundle": ">=4.4",
        "symfony/translation": ">=4.4",
        "symfony/validator": ">=4.4",
        "symfony/maker-bundle": ">=1.0",
        "sensio/framework-extra-bundle": ">=4",
        "doctrine/annotations": ">=1.0",
        "kevinpapst/adminlte-bundle": ">=3.2",
        "cnam/twigbootstrap-bundle": ">=4.0"
    },
    "autoload": {
        "psr-4": { "Sbyaute\\TwigBootstrapMakerBundle\\": "src/" }
    }
}
